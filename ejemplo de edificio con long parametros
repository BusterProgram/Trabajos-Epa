programa ejem6
procesos
  proceso cuadrado (E lado : numero)
  comenzar
    repetir 4
      repetir lado
        mover
      derecha
  fin
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  variables
    long : numero
  comenzar
    long:= 5
    cuadrado (long)
    long:= long -2
    Pos(PosAv+1,PosCa+5)
    cuadrado (long)
    long:= long -2
    Pos(PosAv+1,PosCa+3)
    cuadrado(long)
  fin
variables
  R-info:robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin