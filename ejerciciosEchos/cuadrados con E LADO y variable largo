programa LimpiarEsquina
procesos
  proceso cuadrado (E lado: numero)
  comenzar
    repetir 4
      repetir lado
        mover
      derecha
  fin
areas
  ciudad: AreaC (1,1,100,100)
robots
  robot robot1
  variables
    largo: numero
  comenzar
    largo:= 1
    repetir 4
      cuadrado(largo)
      largo:= largo+1    
  fin
variables
  Rinfo: robot1
comenzar
  AsignarArea (Rinfo,ciudad)
  Iniciar (Rinfo,1,1)
fin