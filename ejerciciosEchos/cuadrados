  robot robot1
  variables
    vacias: numero
  comenzar
    Pos(1,1)
    cuadrado
    Pos(PosAv+1,PosCa+2)
    cuadrado
    Pos(PosAv+1,PosCa+2)
    cuadrado
  fin
variables
  R-Info: robot1
comenzar
  AsignarArea(R-Info,ciudad)
  Iniciar(R-Info,1,1)
fin
