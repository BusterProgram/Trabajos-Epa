programa ejem6
procesos
  proceso escalon (E base : numero ; E altura : numero )
  comenzar
    repetir base
      mover
      derecha
    repetir altura
      mover
    repetir 3
      derecha
  fin
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  variables
    long : numero
  comenzar
    long:= 2
    repetir 3
      escalon (1,long)
      long:= long + 1
  fin
variables
  R-info:robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin